//Complete sprite
void garchomp(float X, float Y) {
  
  //Changeable variables
  int size = 2;
  int xPos = int(X);
  int yPos = int(Y);
  boolean isShiny = false;
  
  if(isShiny == false) {
    noStroke();
    gar(size,xPos, yPos, #394183,#7b7bcd,#5a62ac,#292952,#c59410,#ffd518,#101010,#ffffff,#c5cdd5,#737b83,#6a4a29,#bd737b,#e64a31,#bd3941,#5a1000);
    chomp(size, xPos, yPos, #394183,#7b7bcd,#5a62ac,#292952,#c59410,#ffd518,#101010,#ffffff,#c5cdd5,#737b83,#6a4a29,#bd737b,#e64a31,#bd3941,#5a1000);
  }
  else {
    noStroke();
    gar(size,xPos, yPos, #394173,#738bbd,#5a6a9c,#182039,#c59410,#ffd518,#101010,#ffffff,#c5cdd5,#737b83,#6a4a29,#bd737b,#de5a29,#ac3910,#5a1000);
    chomp(size,xPos, yPos, #394173,#738bbd,#5a6a9c,#182039,#c59410,#ffd518,#101010,#ffffff,#c5cdd5,#737b83,#6a4a29,#bd737b,#de5a29,#ac3910,#5a1000);
  }
}

